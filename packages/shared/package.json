{
  "name": "@fancytrader/shared",
  "version": "0.0.1",
  "private": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "dev": "tsup src/index.ts --dts --format esm,cjs --watch --sourcemap",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "rimraf": "5.0.5",
    "tsup": "^8.5.0",
    "typescript": "5.6.3"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./server": {
      "types": "./src/env.server.ts",
      "default": "./src/env.server.ts"
    },
    "./client": {
      "types": "./src/env.client.ts",
      "default": "./src/env.client.ts"
    }
  },
  "typesVersions": {
    "*": {
      "server": ["src/env.server.ts"],
      "client": ["src/env.client.ts"]
    }
  },
  "sideEffects": false
}
