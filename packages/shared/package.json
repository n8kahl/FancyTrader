{
  "name": "@fancytrader/shared",
  "version": "0.0.1",
  "private": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup -c",
    "dev": "tsup src/index.ts src/env.server.ts src/env.client.ts src/client/massive.ts --dts --format esm,cjs --watch --sourcemap",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "rimraf": "5.0.5",
    "tsup": "^8.5.0",
    "typescript": "5.6.3"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./server": {
      "types": "./dist/env.server.d.ts",
      "import": "./dist/env.server.js",
      "require": "./dist/env.server.cjs"
    },
    "./env.server": {
      "types": "./dist/env.server.d.ts",
      "import": "./dist/env.server.js",
      "require": "./dist/env.server.cjs"
    },
    "./client": {
      "types": "./dist/env.client.d.ts",
      "import": "./dist/env.client.js",
      "require": "./dist/env.client.cjs"
    },
    "./client/massive": {
      "types": "./dist/client/massive.d.ts",
      "import": "./dist/client/massive.js",
      "require": "./dist/client/massive.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "server": ["dist/env.server.d.ts"],
      "env.server": ["dist/env.server.d.ts"],
      "client": ["dist/env.client.d.ts"],
      "client/massive": ["dist/client/massive.d.ts"]
    }
  },
  "sideEffects": false
}
