{
  "name": "@fancytrader/shared",
  "version": "0.0.1",
  "private": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "rimraf": "5.0.5",
    "tsup": "^8.0.1",
    "typescript": "5.6.3"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./cjs": {
      "types": "./dist/index.d.cts",
      "import": "./dist/index.cjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.cjs"
    }
  }
}
